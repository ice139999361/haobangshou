<select id="CustOrderDetail_listtotalMoneyByPeriod" resultMap="custOrderDetail_result" parameterClass="custOrderDetail">
<![CDATA[
	SELECT
		C_CODE,
		PO_NO,
		PO_NO_TYPE,
		SUM(MONEY) AS MONEY
	FROM T_CUST_ORDER_DETAIL
	WHERE C_CODE=#commCode# AND PO_NO=#poNo# AND PERIOD=#period# AND STATE !='03'
	GROUP BY C_CODE,PO_NO,PO_NO_TYPE
]]>

</select>

