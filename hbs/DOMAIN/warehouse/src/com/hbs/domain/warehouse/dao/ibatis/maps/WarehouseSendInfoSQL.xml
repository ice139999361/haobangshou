<insert id="WarehouseSendInfo_insertWarehouseSendInfo" parameterClass="warehouseSendInfo" >
<![CDATA[
    INSERT INTO T_WAREHOUSE_SEND (
            SEND_PO_NO, 
            C_CODE, 
            SHORT_NAME, 
            RECEIVE_NAME, 
            RECEIVE_ADDRESS, 
            RECEIVE_ZIP, 
            CON_TEL, 
            CON_FAX, 
            COMPANY_BRANCH, 
            HOUSE_TYPE, 
            SETTLEMENT_TYPE, 
            CREATE_DATE, 
            OPER_ID, 
            OPER_STAFF, 
            SEND_DESC, 
            PERIOD, 
            STATE, 
            ACTIVE_STATE, 
            FINANCE_STATE, 
            PO_NO_TYPE
        )
    VALUES (
            #sendPoNo#,
            #custCode#,
            #shortName#,
            #receiveName#,
            #receiveAddress#,
            #receiveZip#,
            #conTel#,
            #conFax#,
            #companyBranch#,
            #houseType#,
            #settlementType#,
            #createDate#,
            #operId#,
            #operStaff#,
            #sendDesc#,
            #period#,
            #state#,
            #activeState#,
            #financeState#,
            #poNoType#
    )
]]>
</insert>

<delete id="WarehouseSendInfo_deleteWarehouseSendInfo" parameterClass="string">
<![CDATA[
    DELETE  FROM  T_WAREHOUSE_SEND WHERE SEND_PO_NO = #sendPoNo# AND C_CODE=#custCode#
]]>
</delete>    

