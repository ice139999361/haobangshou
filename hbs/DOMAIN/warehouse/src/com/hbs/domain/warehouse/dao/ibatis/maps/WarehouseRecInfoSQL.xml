<insert id="WarehouseRecInfo_insertWarehouseRecInfo" parameterClass="warehouseRecInfo" >
<![CDATA[
    INSERT INTO T_WAREHOUSE_RECEIVE (
            REC_PO_NO, 
            C_CODE, 
            SHORT_NAME, 
            PO_NO_DATE, 
            ARRAY_DATE, 
            HOUSE_TYPE, 
            OPER_ID, 
            OPER_STAFF, 
            OPER_TIME, 
            RECEIVE_DESC, 
            PERIOD, 
            STATE, 
            ACTIVE_STATE, 
            FINANCE_STATE, 
            PO_NO_TYPE,
			SETTLEMENT_TYPE
        )
    VALUES (
            #recPoNo#,
            #vendorCode#,
            #shortName#,
            #poNoDate#,
            #applyDate#,
            #houseType#,
            #operId#,
            #operStaff#,
            #operTime#,
            #receiveDesc#,
            #period#,
            #state#,
            #activeState#,
            #financeState#,
            #poNoType#,
			#settlementType#
    )
]]>
</insert>

<delete id="WarehouseRecInfo_deleteWarehouseRecInfo" parameterClass="string">
<![CDATA[
    DELETE  FROM  T_WAREHOUSE_RECEIVE WHERE REC_PO_NO = #recPoNo# AND C_CODE = #vendorCode#
]]>
</delete>    
<update id="WarehouseRecInfo_updateWarehouseRecInfoByState" parameterClass="warehouseRecInfo">
	UPDATE T_WAREHOUSE_RECEIVE 
    SET
    	STATE = #state#
	
	<dynamic prepend="WHERE">
			<isNotEmpty prepend="and" property = "recPoNo">
				REC_PO_NO = #recPoNo#
			</isNotEmpty>
			<isNotEmpty prepend="and" property = "vendorCode">
				C_CODE = #vendorCode#
			</isNotEmpty>			
		</dynamic>
</update>

