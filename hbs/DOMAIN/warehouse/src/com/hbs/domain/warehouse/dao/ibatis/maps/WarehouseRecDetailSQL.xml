			</isNotEmpty>
			<isNotEmpty prepend="," property="state">
		            STATE = #state#
			</isNotEmpty>
			<isNotEmpty prepend="," property="activeState">
		            ACTIVE_STATE = #activeState#
			</isNotEmpty>
			<isNotEmpty prepend="," property="financePeriod">
