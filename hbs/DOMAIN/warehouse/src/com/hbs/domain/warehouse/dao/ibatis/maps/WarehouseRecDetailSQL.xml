				]]>
			</isNotEmpty>
			<isNotEmpty prepend="and"
				property="dynamicFields.endApplyDate">
				<![CDATA[	
					ORDER_TIME < STR_TO_DATE('$dynamicFields.endApplyDate$','%Y-%m-%d')
				]]>
			</isNotEmpty>
			
			<isNotEmpty prepend="order by" property="sort">
                   $sort$
            </isNotEmpty>
		</dynamic>
		<![CDATA[
      		LIMIT #start# ,#end#
		]]>
</select>


