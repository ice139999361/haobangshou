			</isNotEmpty>
			<isNotEmpty prepend="and" property = "specDesc">
				SPEC_DESC = #specDesc#
			</isNotEmpty>
			
			<!-- 账期范围查询  需要extends BaseDomain 入库单供应商日期 -->	
			<isNotEmpty prepend="and"
				property="dynamicFields.lessPeriod">
				<![CDATA[	
					CAST(PERIOD AS SIGNED) <= #lessPeriod#
				]]>
			</isNotEmpty>
		<!-- 时间段范围查询  需要extends BaseDomain 入库单供应商日期 -->
			<isNotEmpty prepend="and"
				property="dynamicFields.beginPoNoDate">
				<![CDATA[	
					ORDER_TIME >= STR_TO_DATE('$dynamicFields.beginPoNoDate$','%Y-%m-%d')
				]]>
			</isNotEmpty>
			<isNotEmpty prepend="and"
				property="dynamicFields.endPoNoDate">
				<![CDATA[	
					ORDER_TIME < STR_TO_DATE('$dynamicFields.endPoNoDate$','%Y-%m-%d')
				]]>
			</isNotEmpty>
			<isNotEmpty prepend="and"
