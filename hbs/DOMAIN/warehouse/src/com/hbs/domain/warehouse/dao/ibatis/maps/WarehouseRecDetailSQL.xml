            C_CODE, 
            SHORT_NAME, 
            PO_NO_DATE, 
            ARRAY_DATE, 
            HOUSE_TYPE, 
            RLT_PO_NO, 
            SETTLEMENT_TYPE, 
            PART_NO, 
            C_PART_NO, 
            PN_DESC, 
            C_PRICE, 
            C_PRICE_TAX,
            IS_TAX, 
            TAX_RATE, 
            AMOUNT,
			CUR_MONEY, 
            PERIOD, 
            FINANCE_STATE, 
            PO_NO_TYPE,
			STAFF_ID,
			STAFF_NAME,
			OPER_TIME,
			STATE,
