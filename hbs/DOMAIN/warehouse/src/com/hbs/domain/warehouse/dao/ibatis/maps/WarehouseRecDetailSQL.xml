<insert id="WarehouseRecDetail_insertWarehouseRecDetail" parameterClass="warehouseRecDetail" >
<![CDATA[
    INSERT INTO T_WAREHOUSE_RECEIVE_DETAIL (             
            REC_PO_NO, 
            C_CODE, 
            SHORT_NAME, 
            PO_NO_DATE, 
            ARRAY_DATE, 
            HOUSE_TYPE, 
            RLT_PO_NO, 
            SETTLEMENT_TYPE, 
            PART_NO, 
            C_PART_NO, 
            PN_DESC, 
            C_PRICE, 
            IS_TAX, 
            TAX_RATE, 
            AMOUNT, 
            PERIOD, 
            FINANCE_STATE, 
            PO_NO_TYPE,
			STAFF_ID,
			STAFF_NAME,
			OPER_TIME,
			STATE,
			ACTIVE_STATE,
			FINANCE_PERIOD
        )
    VALUES (            
            #recPoNo#,
            #vendorCode#,
            #shortName#,
            #poNoDate#,
            #applyDate#,
            #houseType#,
            #rltPoNo#,
            #settlementType#,
            #partNo#,
            #venPartNo#,
            #pnDesc#,
            #price#,
            #isTax#,
            #taxRate#,
            #amount#,
            #period#,
            #financeState#,
            #poNoType#,
