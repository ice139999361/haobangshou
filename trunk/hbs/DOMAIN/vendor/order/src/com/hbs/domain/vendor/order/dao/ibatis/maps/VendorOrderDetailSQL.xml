<insert id="VendorOrderDetail_insertVendorOrderDetail" parameterClass="vendorOrderDetail" >
<![CDATA[
    INSERT INTO T_VENDOR_ORDER_DETAIL (            
            C_CODE, 
            SHORT_NAME, 
            PO_NO_TYPE, 
            PO_NO, 
            C_PART_NO, 
            PART_NO, 
            PN_DESC, 
            C_PRICE,
            C_PRICE_TAX, 
            IS_TAX, 
            TAX_RATE, 
            SPEC_DESC, 
            COMM_DESC, 
            AMOUNT, 
            MONEY, 
            DELIVERY_AMOUNT, 
            ORG_DELIVERY_DATE, 
            VER_DELIVERY_DATE, 
            PERIOD, 
            RLT_ORDER_PO_NO, 
            STATE, 
            ACTIVE_STATE, 
            RLT_REC_PO_NO,
			SETTLEMENT_TYPE,
            STAFF_ID,
            STAFF_NAME,
            CUST_C_CODE
        )
    VALUES (           
            #commCode#,
            #shortName#,
            #poNoType#,
            #poNo#,
            #cpartNo#,
            #partNo#,
            #pnDesc#,
            #cprice#,
            #cpriceTax#,
            #isTax#,
            #taxRate#,
            #specDesc#,
            #commDesc#,
            #amount#,
            #money#,
            #deliveryAmount#,
            #orgDeliveryDate#,
            #verDeliveryDate#,
            #period#,
            #rltOrderPoNo#,
            #state#,
            #activeState#,
            #rltRecPoNo#,
			#settlementType#,
            #staffId#,
            #staffName#,
            #custCcode#
    )
]]>
 <selectKey resultClass="Integer" keyProperty="operSeqId">
            SELECT LAST_INSERT_ID()  
  </selectKey>
</insert>

<delete id="VendorOrderDetail_deleteVendorOrderDetail" parameterClass="string">
<![CDATA[
