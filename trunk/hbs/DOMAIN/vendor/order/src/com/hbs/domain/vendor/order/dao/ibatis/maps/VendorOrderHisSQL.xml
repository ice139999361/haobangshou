            RECEIVE_ZIP = #receiveZip#
	</isNotEmpty>
	<isNotEmpty prepend="," property="staffId">
            STAFF_ID = #staffId#
	</isNotEmpty>
	<isNotEmpty prepend="," property="staffName">
            STAFF_NAME = #staffName#
	</isNotEmpty>
	<isNotEmpty prepend="," property="sales">
            SALES = #sales#
	</isNotEmpty>
	<isNotEmpty prepend="," property="isShowPrice">
