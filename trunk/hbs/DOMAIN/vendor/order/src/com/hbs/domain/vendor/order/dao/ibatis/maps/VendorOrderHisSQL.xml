<insert id="VendorOrderHis_insertVendorOrder" parameterClass="vendorOrder" >
<![CDATA[
    INSERT INTO T_VENDOR_ORDER (
            C_CODE, 
            PO_NO, 
            PO_NO_TYPE, 
            SHORT_NAME, 
            CREATE_TIME, 
            CON_NAME, 
            CON_TEL, 
            CON_FAX, 
            COMPANY_BRANCH, 
            SETTLEMENT_TYPE, 
            RECEIVE_NAME, 
            RECEIVE_ADDRESS, 
            RECEIVE_ZIP, 
            STAFF_ID, 
            STAFF_NAME, 
            SALES, 
            IS_SHOW_PRICE, 
            ORDER_TIME, 
            PERIOD, 
            STATE, 
            ACTIVE_STATE,
            CUST_C_CODE
        )
    VALUES (
            #commCode#,
            #poNo#,
            #poNoType#,
            #shortName#,
            #createTime#,
            #conName#,
            #conTel#,
            #conFax#,
            #companyBranch#,
            #settlementType#,
            #receiveName#,
            #receiveAddress#,
            #receiveZip#,
            #staffId#,
            #staffName#,
            #sales#,
            #isShowPrice#,
            #orderTime#,
            #period#,
            #state#,
            #activeState#,
            #custCcode#
    )
]]>
</insert>

<delete id="VendorOrderHis_deleteVendorOrder" parameterClass="string">
<![CDATA[
    DELETE  FROM  T_VENDOR_ORDER WHERE C_CODE = #commCode#
]]>
</delete>    

<update id="VendorOrderHis_updateVendorOrderByActiveState" parameterClass="vendorOrder">
<![CDATA[
    UPDATE T_VENDOR_ORDER
    SET ACTIVE_STATE = #activeState# 
    WHERE C_CODE = #commCode# AND PO_NO = #poNo#
]]>	
</update>

<update id="VendorOrderHis_updateVendorOrderByState" parameterClass="vendorOrder">
	<![CDATA[
	    UPDATE T_VENDOR_ORDER
	    SET STATE = #state#     
	]]>	
	<dynamic prepend="WHERE">					
					<isNotEmpty prepend="and" property = "commCode">
						C_CODE = #commCode#
					</isNotEmpty>
					<isNotEmpty prepend="and" property = "poNo">
						PO_NO = #poNo#
					</isNotEmpty>
					<isNotEmpty prepend = "and" property = "dynamicFields.state" >
						<![CDATA[ 
						  STATE != '%$dynamicFields.state$%'
						]]> 
					</isNotEmpty>
		</dynamic>
</update>

