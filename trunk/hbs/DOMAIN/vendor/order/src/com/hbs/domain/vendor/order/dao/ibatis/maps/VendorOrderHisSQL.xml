            SHORT_NAME, 
            CREATE_TIME, 
            CON_NAME, 
            CON_TEL, 
            CON_FAX, 
            COMPANY_BRANCH, 
            SETTLEMENT_TYPE, 
            RECEIVE_NAME, 
            RECEIVE_ADDRESS, 
            RECEIVE_ZIP, 
            STAFF_ID, 
            STAFF_NAME, 
            SALES, 
            IS_SHOW_PRICE, 
            ORDER_TIME, 
            PERIOD, 
            STATE, 
            ACTIVE_STATE,
            CUST_C_CODE
    FROM T_VENDOR_ORDER WHERE C_CODE = #commCode# AND PO_NO = #poNo#
]]>
