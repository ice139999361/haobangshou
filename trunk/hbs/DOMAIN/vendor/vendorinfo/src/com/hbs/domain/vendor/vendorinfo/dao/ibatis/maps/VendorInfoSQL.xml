<insert id="VendorInfo_insertVendorInfo" parameterClass="vendorInfo" >
<![CDATA[
    INSERT INTO T_VENDOR_INFO (
            C_CODE, 
            STATE, 
            SHORT_NAME, 
            EN_SHORT_NAME, 
            ALL_NAME, 
            EN_NAME, 
            ADDRESS, 
            EN_ADDRESS, 
            C_TYPE, 
            C_SCALE, 
            WEBSITE, 
            REPRESENTATIVE, 
            TAX_CODE, 
            COMPANY_BRANCH, 
            CREDIT_RATE, 
            CREDIT_DESC, 
            IMPORTANT_CODE, 
            IMPORTANT_DESC, 
            SETTLEMENT_TYPE, 
            SETTLEMENT_DESC, 
            CURRENCY, 
            CURRENCY_DESC, 
            STAFF_ID, 
            STAFF_NAME, 
            COMM_DESC,             
            CONTACT_FEE, 
            IS_SHOW_PRICE, 
            TAX_RATE,
			CREATE_TIME
        )
    VALUES (
            #commCode#,
            #state#,
            #shortName#,
            #enShortName#,
            #allName#,
            #enName#,
            #address#,
            #enAddress#,
            #commType#,
            #commScale#,
            #webSite#,
            #representative#,
            #taxCode#,
            #companyBranch#,
            #creditRate#,
            #creditDesc#,
            #importantCode#,
            #importantDesc#,
            #settlementType#,
            #settlementDesc#,
            #currency#,
            #currencyDesc#,
            #staffId#,
            #staffName#,
            #commDesc#,           
            #contactFee#,
            #isShowPrice#,
            #taxRate#,
			#createTime#
    )
]]>

<selectKey resultClass="Integer" keyProperty="baseSeqId">
            SELECT LAST_INSERT_ID()  
</selectKey>
</insert>

<delete id="VendorInfo_deleteVendorInfoByID" parameterClass="string">
<![CDATA[
    DELETE  FROM  T_VENDOR_INFO WHERE BASE_SEQID = #baseSeqId#
]]>
</delete>

<delete id="VendorInfo_deleteVendorInfoByBase" parameterClass="vendorInfo">
<![CDATA[
    DELETE  FROM  T_VENDOR_INFO 
]]>
	<dynamic prepend="WHERE">
		<isNotEmpty prepend="and" property="commCode">
			C_CODE = #commCode#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="state">
			STATE = #state#
		</isNotEmpty>
	</dynamic>
</delete>    

<update id="VendorInfo_updateVendorInfoByState" parameterClass="vendorInfo">
	UPDATE T_VENDOR_INFO 
		SET STATE =#state#
	WHERE BASE_SEQID = #baseSeqId#
</update>

