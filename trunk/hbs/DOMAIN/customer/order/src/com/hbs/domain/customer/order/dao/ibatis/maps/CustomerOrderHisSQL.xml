            PO_NO_TYPE, 
            SHORT_NAME, 
            ORDER_TIME, 
            CON_NAME, 
            CON_TEL, 
            CON_FAX, 
            COMPANY_BRANCH, 
            SETTLEMENT_TYPE, 
            RECEIVE_NAME, 
            RECEIVE_ADDRESS, 
            RECEIVE_ZIP, 
            STAFF_ID, 
            STAFF_NAME, 
            SALES_ID, 
            SALES, 
            IS_SHOW_PRICE, 
            VENDOR_CODE, 
            FIRST_CREATE_TIME, 
            CREATE_TIME, 
            PERIOD, 
            STATE, 
