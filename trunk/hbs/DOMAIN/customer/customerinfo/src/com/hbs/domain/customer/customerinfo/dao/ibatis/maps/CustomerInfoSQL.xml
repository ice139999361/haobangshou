	<insert id="CustomerInfo_insertCustomerInfo" parameterClass="customerInfo">
<![CDATA[
    INSERT INTO T_CUSTOMER_INFO (
            C_CODE, 
            STATE, 
            SHORT_NAME, 
            EN_SHORT_NAME, 
            ALL_NAME, 
            EN_NAME, 
            ADDRESS, 
            EN_ADDRESS, 
            C_TYPE, 
            C_SCALE, 
            WEBSITE, 
            REPRESENTATIVE, 
            TAX_CODE, 
            COMPANY_BRANCH, 
            CREDIT_RATE, 
            CREDIT_DESC, 
            IMPORTANT_CODE, 
            IMPORTANT_DESC, 
            SETTLEMENT_TYPE, 
            SETTLEMENT_DESC, 
            CURRENCY, 
            CURRENCY_DESC, 
            STAFF_ID, 
            STAFF_NAME, 
            COMM_DESC, 
            VENDOR_CODE, 
            CONTACT_FEE, 
            IS_SHOW_PRICE, 
            TAX_RATE, 
            ASS_STAFF_ID, 
            ASS_STAFF_NAME,
			CREATE_TIME
        )
    VALUES (
            #commCode#,
            #state#,
            #shortName#,
            #enShortName#,
            #allName#,
            #enName#,
            #address#,
            #enAddress#,
            #commType#,
            #commScale#,
            #webSite#,
            #representative#,
            #taxCode#,
            #companyBranch#,
            #creditRate#,
            #creditDesc#,
            #importantCode#,
            #importantDesc#,
            #settlementType#,
            #settlementDesc#,
            #currency#,
            #currencyDesc#,
            #staffId#,
            #staffName#,
            #commDesc#,
            #vendorCode#,
            #contactFee#,
            #isShowPrice#,
            #taxRate#,
            #assStaffId#,
            #assStaffName#,
			#createTime#
    )
]]>
  <selectKey resultClass="Integer" keyProperty="baseSeqId">
            SELECT LAST_INSERT_ID()  
  </selectKey>
	</insert>

<delete id="CustomerInfo_deleteCustomerInfoByBase" parameterClass="customerInfo">
	<![CDATA[
	    DELETE  FROM  T_CUSTOMER_INFO
	]]>
	<dynamic prepend="WHERE">
		<isNotEmpty prepend="and" property="commCode">
			C_CODE = #commCode#
		</isNotEmpty>
		<isNotEmpty prepend="and" property="state">
			STATE = #state#
		</isNotEmpty>
	</dynamic>
</delete>
<delete id="CustomerInfo_deleteCustomerInfoByID" parameterClass="String">
	<![CDATA[
	    DELETE  FROM  T_CUSTOMER_INFO WHERE BASE_SEQID = #baseSeqId#
	]]>	
</delete>

<update id="CustomerInfo_updateCustomerInfoByState" parameterClass="customerInfo">
	UPDATE T_CUSTOMER_INFO
	SET STATE =#state#
	WHERE BASE_SEQID = #baseSeqId#
</update>

