            SHORT_NAME, 
            PO_NO_DATE, 
            ARRAY_DATE, 
            HOUSE_TYPE, 
            OPER_ID, 
            OPER_STAFF, 
            OPER_TIME, 
            RECEIVE_DESC, 
            PERIOD, 
            LOCK_AMOUNT, 
            ACTIVE_STATE, 
            FINANCE_STATE, 
            PO_NO_TYPE,
			SETTLEMENT_TYPE
        )
    VALUES (
            #recPoNo#,
            #vendorCode#,
            #shortName#,
            #poNoDate#,
            #applyDate#,
            #houseType#,
            #operId#,
            #operStaff#,
            #operTime#,
            #receiveDesc#,
            #period#,
            #state#,
            #activeState#,
            #financeState#,
            #poNoType#,
